    url('/fonts/aasaam-NotoSansDisplay-Bold.woff2') format('woff2'),
    url('/fonts/aasaam-NotoSansDisplay-Bold.woff') format('woff'),
    url('/fonts/aasaam-NotoSansDisplay-Bold.ttf') format('ttf');
}
