.theme-container.custom-landing {
  /* page-specific rules */
  background-color: #f0f0f0;
  font-family: "aasaam-Noto", sans-serif !important;
}
@font-face {
  font-family: 'aasaam-Noto';
  font-display: swap;
  font-weight: 700;
